<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-msi">
    <comment>Windows Installer Package</comment>
    <sub-class-of type="application/x-ole-storage"/>
    <glob pattern="*.msi"/>
  </mime-type>
  <mime-type type="application/x-ms-shortcut">
    <comment>Windows Shortcut</comment>
    <glob pattern="*.lnk"/>
  </mime-type>
  <mime-type type="application/x-chm">
    <comment>Compiled HTML Help Format</comment>
    <glob pattern="*.chm"/>
  </mime-type>
  <mime-type type="application/x-winhelp">
    <comment>Windows Help File</comment>
    <glob pattern="*.hlp"/>
  </mime-type>
  <mime-type type="application/x-bat">
    <comment>Batch File</comment>
    <sub-class-of type="text/plain"/>
    <glob pattern="*.bat"/>
  </mime-type>
</mime-info>

